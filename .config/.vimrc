set number
set mouse=a
set numberwidth=1
set clipboard=unnamed
syntax enable
set showcmd
set ruler
set encoding=utf-8
set showmatch
set shiftwidth=4
set relativenumber
set laststatus=2
set noshowmode
set shell=sh " nerdtree-git-plugin
set smarttab
set cindent
set tabstop=4
set expandtab " always uses spaces instead of tab characters
set t_Co=256
set t_ut=
" End of setters

so ~/.vim/plugins.vim
so ~/.vim/plugin-config.vim
so ~/.vim/maps.vim
" End of sources

